{
  "name": "frontend",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "pnpm run codegen; astro build",
    "preview": "wrangler pages dev ./dist --compatibility-date=2023-10-30 --compatibility-flags=\"nodejs_compat\"",
    "astro": "astro",
    "codegen": "sanity-codegen && pnpm run i18n:sync",
    "i18n:install": "astro-i18n install",
    "i18n:extract": "astro-i18n extract",
    "i18n:generate:pages": "astro-i18n generate:pages --purge",
    "i18n:generate:types": "astro-i18n generate:types",
    "i18n:sync": "pnpm run i18n:generate:pages && pnpm run i18n:generate:types"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^8.0.0",
    "@astrojs/react": "^3.0.7",
    "@astrojs/sitemap": "^3.0.3",
    "@fontsource/noto-serif-kr": "^5.0.11",
    "@sanity-codegen/types": "1.0.0-alpha.13",
    "@sanity/astro": "^2.1.4",
    "@sanity/client": "^6.10.0",
    "@sanity/dashboard": "^3.1.6",
    "@sanity/document-internationalization": "^2.0.2",
    "@sanity/eslint-config-studio": "^3.0.1",
    "@sanity/image-url": "^1.0.2",
    "@sanity/language-filter": "^3.2.1",
    "@sanity/preview-url-secret": "~1.3.0",
    "@sanity/ui": "^1.9.3",
    "@sanity/uuid": "^3.0.2",
    "@sanity/vision": "~3.22.0",
    "@types/react": "^18.2.45",
    "astro": "^3.5.0",
    "astro-font": "^0.0.28",
    "astro-i18n": "^2.2.0",
    "astro-icon": "^1.0.0",
    "astro-portabletext": "^0.9.6",
    "astro-robots-txt": "^1.0.0",
    "astro-sanity-picture": "^0.2.0",
    "astro-seo": "^0.8.0",
    "eslint": "^8.56.0",
    "fuse.js": "^7.0.0",
    "groq": "^3.21.3",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-is": "^18.2.0",
    "rxjs": "^7.8.1",
    "sanity": "~3.22.0",
    "sanity-codegen": "^0.9.8",
    "sanity-plugin-iframe-pane": "^3.1.3",
    "sanity-plugin-internationalized-array": "^1.10.3",
    "sanity-plugin-media": "^2.2.4",
    "sass": "^1.69.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "wrangler": "^3.22.1"
  },
  "devDependencies": {
    "prettier-plugin-astro": "^0.12.2"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "tty": false,
    "child_process": false,
    "crypto": false
  }
}
